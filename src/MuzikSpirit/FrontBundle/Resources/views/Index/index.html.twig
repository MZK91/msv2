                            {% set i = 1 %}
                            {% set display = 1 %}
                            {% for item in carousel %}
                                {% if loop.length >= 3 and display == 1 %}
                                {% if i == 1 %}
                                <div class="slider-item">
                                    <div class="row">
                                        <div class="col-md-8 omega">
                                            <div class="featured-big">
                                                <a href="{{ item.lien }}" class="featured-href">
                                                    <img src="{{ asset(item.image.typeImage.path ~ item.image.image) }}" alt="{{ item.titre }}"/>
                                                    <div class="featured-header">
                                                        <span class="category bgcolor2">{{ item.typeArticle.titre }}</span>
                                                        <h2>{{ item.titre }}</h2>
                                                        <p class="simple-share">
                                                            {{ item.description }}
                                                        </p>
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                {% endif %}
                                {% if i == 2 %}
                                        <div class="col-md-4 alpha">
                                            <div class="featured-small featured-top">
                                                <a href="#" class="featured-href">
                                                    <img src="{{ asset(item.image.typeImage.path) ~ 'thumbs/' ~ item.image.image }}" alt=""/>
                                                    <div class="featured-header">
                                                        <span class="category bgcolor5">{{ item.typeArticle.titre }}</span>
                                                        <h2>{{ item.titre }}</h2>
                                                        <p class="simple-share">
                                                            {{ item.description }}
                                                        </p>
                                                    </div>
                                                </a>
                                            </div>
                                {% endif %}
                                {% if i == 3 %}
                                            <div class="featured-small">
                                                <a href="#" class="featured-href">
                                                    <img src="{{ asset(item.image.typeImage.path) ~ 'thumbs/' ~ item.image.image }}" alt=""/>
                                                    <div class="featured-header">
                                                        <span class="category bgcolor4">{{ item.typeArticle.titre }}</span>
                                                        <h2>{{ item.titre }}</h2>
                                                        <p class="simple-share">
                                                            {{ item.description }}
                                                        </p>
                                                    </div>
                                                </a>
                                            </div>
                                        </div>
                                {% endif %}
                                {% if i == 3 %}
                                    </div><!-- .row -->
                                    </div><!-- .slider-item -->
                                    {% if loop.revindex <= 3 %}
                                        {% set display = 0 %}
                                    {% endif %}
                                    {% set i = 0 %}
                                {% endif %}
                                {% set i = i + 1 %}
                            {% endif %}
                            {% endfor %}
                            </div><!-- .featured-slider -->
