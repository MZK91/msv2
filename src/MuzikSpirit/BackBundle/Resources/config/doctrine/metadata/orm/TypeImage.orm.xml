<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MuzikSpirit\BackBundle\Entity\TypeImage" table="type_image">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="type" type="string" column="type" length="255" nullable="false"/>
    <field name="crop" type="boolean" column="crop" nullable="false"/>
    <field name="thumb" type="boolean" column="thumb" nullable="false"/>
    <field name="width" type="integer" column="width" nullable="false"/>
    <field name="height" type="integer" column="height" nullable="false"/>
    <field name="path" type="string" column="path" length="255" nullable="false"/>
    <field name="resize" type="boolean" column="resize" nullable="false"/>
  </entity>
</doctrine-mapping>
