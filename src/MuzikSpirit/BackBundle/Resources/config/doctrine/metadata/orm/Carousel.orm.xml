<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Carousel" table="carousel">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="sectionId" type="integer" column="section_id" nullable="false"/>
    <field name="typeArticleId" type="integer" column="type_article_id" nullable="false"/>
    <field name="articleId" type="integer" column="article_id" nullable="false"/>
    <field name="titre" type="string" column="titre" length="255" nullable="false"/>
    <field name="image" type="string" column="image" length="255" nullable="false"/>
    <field name="lien" type="string" column="lien" length="255" nullable="false"/>
    <field name="description" type="string" column="description" length="255" nullable="true"/>
  </entity>
</doctrine-mapping>
