                        <div class="panel-padding no-padding-vr">
                            {% for item in article %}
                            <div class="ticket">
                                <span class="label label-success ticket-label">{{ item.typeArticle.titre }}</span>
                                <a href="http://{{ global_website_url ~ '/' ~ item.typeArticle.url ~ '/' ~ item.slug ~ '/' ~ item.articleId }}" title="" class="ticket-title">{{ item.titre }}<span>[#{{ item.articleId }}]</span></a>
								<span class="ticket-info">
									 {{ item.date | date("d/m/Y")  }}
								</span>
                            </div> <!-- / .ticket -->
                            {% endfor %}
